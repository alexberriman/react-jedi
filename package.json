{
  "name": "@alexberriman/react-jedi",
  "version": "1.0.5",
  "description": "Rapidly build beautiful, modern websites using structured configurations, React, TailwindCSS, and ShadCN components.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "preview": "vite preview",
    "test": "vitest run --changed",
    "test:all": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "prepare": "husky",
    "dev:setup": "playwright install chromium",
    "lint": "eslint src/ --fix --cache --cache-location node_modules/.cache/eslint/",
    "lint:fix": "eslint src/ --fix --cache --cache-location node_modules/.cache/eslint/",
    "lint:example-app": "eslint examples/ --fix --cache --cache-location node_modules/.cache/eslint/",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" --cache --cache-location node_modules/.cache/prettier/",
    "format:check": "prettier --check src/ --cache --cache-location node_modules/.cache/prettier/",
    "typecheck": "tsc --noEmit",
    "check": "concurrently --kill-others-on-fail -n 'LINT,TYPECHECK,TEST' -c 'yellow,cyan,magenta' \"npm:lint\" \"npm:typecheck\" \"npm:test\"",
    "lint-staged": "lint-staged",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "kill-storybook": "kill-port 6006",
    "test-storybook": "./scripts/test-storybook.sh",
    "test-storybook:only": "test-storybook",
    "test-storybook:ci": "npm run kill-storybook && npm run build-storybook && (npx http-server storybook-static -p 6006 -s & npx wait-on http://localhost:6006) && test-storybook --ci --url http://localhost:6006",
    "test-storybook:watch": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run storybook\" \"wait-on tcp:6006 && test-storybook --watch\"",
    "test-storybook:coverage": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run storybook\" \"wait-on tcp:6006 && test-storybook --coverage\"",
    "benchmark": "vitest bench -c benchmark.config.ts",
    "benchmark:ui": "vitest bench -c benchmark.config.ts --ui",
    "benchmark:report": "vitest bench -c benchmark.config.ts --reporter=json --outputFile=./benchmark-results.json",
    "example-app": "npm run build && vite --config examples/vite.config.ts",
    "example-app:build": "npm run build && vite build --config examples/vite.config.ts",
    "build:example": "npm run build && vite build --config examples/vite.config.ts",
    "docker:build": "docker build -f examples/Dockerfile -t ghcr.io/alexberriman/react-jedi-example:latest .",
    "docker:push": "docker push ghcr.io/alexberriman/react-jedi-example:latest",
    "docker:build-push": "npm run docker:build && npm run docker:push",
    "k8s:apply": "kubectl apply -f examples/kubernetes/",
    "k8s:deploy": "npm run docker:build-push && npm run k8s:apply",
    "release:patch": "./scripts/release.sh --patch",
    "release:minor": "./scripts/release.sh --minor",
    "release:major": "./scripts/release.sh --major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexberriman/react-jedi.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexberriman/react-jedi/issues"
  },
  "homepage": "https://github.com/alexberriman/react-jedi#readme",
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@hookform/resolvers": "^3.10.0",
    "@icons-pack/react-simple-icons": "^12.8.0",
    "@radix-ui/react-accordion": "^1.2.10",
    "@radix-ui/react-alert-dialog": "^1.1.13",
    "@radix-ui/react-aspect-ratio": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-collapsible": "^1.1.10",
    "@radix-ui/react-context-menu": "^2.2.14",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-hover-card": "^1.1.13",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-menubar": "^1.1.14",
    "@radix-ui/react-navigation-menu": "^1.2.12",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-progress": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.3.6",
    "@radix-ui/react-scroll-area": "^1.2.8",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slider": "^1.3.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-switch": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-toggle": "^1.1.8",
    "@radix-ui/react-toggle-group": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@storybook/react": "^8.6.14",
    "@tailwindcss/postcss": "^4.1.7",
    "@tailwindcss/vite": "^4.1.6",
    "@tanstack/react-query": "^5.76.1",
    "@tanstack/react-table": "^8.21.3",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "framer-motion": "^12.12.1",
    "glob": "^11.0.2",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.510.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.56.3",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.2",
    "react-router-dom": "^7.6.0",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^2.15.3",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.6",
    "ts-results": "^3.3.0",
    "tslib": "^2.8.1",
    "unified": "^11.0.5",
    "vaul": "^1.1.2",
    "vite": "^6.3.5",
    "zod": "^3.24.4",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@shadcn/ui": "^0.0.4",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/cli": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/test-runner": "^0.22.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.17",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/coverage-v8": "^3.1.3",
    "axe-playwright": "^2.1.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^59.0.1",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "kill-port": "^2.0.1",
    "lint-staged": "^16.0.0",
    "playwright": "^1.49.1",
    "prettier": "^3.5.3",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3",
    "wait-on": "^8.0.3"
  }
}
