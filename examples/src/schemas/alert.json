{
  "basicAlert": {
    "type": "alert",
    "children": [
      {
        "type": "icon",
        "properties": {
          "name": "terminal",
          "className": "h-4 w-4"
        }
      },
      {
        "type": "alert-title",
        "children": "Heads up!"
      },
      {
        "type": "alert-description",
        "children": "You can add components to your app using the cli."
      }
    ]
  },
  "destructiveAlert": {
    "type": "alert",
    "properties": {
      "variant": "destructive"
    },
    "children": [
      {
        "type": "icon",
        "properties": {
          "name": "alert-circle",
          "className": "h-4 w-4"
        }
      },
      {
        "type": "alert-title",
        "children": "Error"
      },
      {
        "type": "alert-description",
        "children": "Your session has expired. Please log in again."
      }
    ]
  },
  "infoAlert": {
    "type": "alert",
    "properties": {
      "className": "border-blue-200 bg-blue-50 dark:border-blue-800 dark:bg-blue-950"
    },
    "children": [
      {
        "type": "icon",
        "properties": {
          "name": "info",
          "className": "h-4 w-4 text-blue-600 dark:text-blue-400"
        }
      },
      {
        "type": "alert-title",
        "properties": {
          "className": "text-blue-900 dark:text-blue-100"
        },
        "children": "Information"
      },
      {
        "type": "alert-description",
        "properties": {
          "className": "text-blue-700 dark:text-blue-300"
        },
        "children": "This is an informational alert using custom colors."
      }
    ]
  },
  "complexContentAlert": {
    "type": "alert",
    "children": [
      {
        "type": "icon",
        "properties": {
          "name": "info",
          "className": "h-4 w-4"
        }
      },
      {
        "type": "alert-title",
        "children": "New Features Available"
      },
      {
        "type": "alert-description",
        "children": [
          {
            "type": "text",
            "properties": {
              "className": "mb-2"
            },
            "children": "We've released several new features:"
          },
          {
            "type": "ul",
            "properties": {
              "className": "list-disc pl-5 space-y-1"
            },
            "children": [
              {
                "type": "li",
                "children": "Enhanced security settings"
              },
              {
                "type": "li",
                "children": "Improved performance metrics"
              },
              {
                "type": "li",
                "children": "New collaboration tools"
              }
            ]
          },
          {
            "type": "text",
            "properties": {
              "className": "mt-2"
            },
            "children": "Visit the documentation to learn more."
          }
        ]
      }
    ]
  }
